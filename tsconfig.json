{
  "compilerOptions": {
   "moduleResolution": "Node",
    "target": "ESNext",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["node", "react", "react-native"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
